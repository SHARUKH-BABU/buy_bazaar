{
  "name": "ecommerce_website",
  "version": "1.0.0",
  "description": "ecommerce website to buy and view cart products, having admin and user roles with authentication",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm install && npm install --prefix client && npm run build --prefix client"
  },
  "author": "sharukh",
  "license": "ISC",
  "dependencies": {
    "antd": "^5.20.1",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "braintree": "^3.24.0",
    "braintree-web-drop-in-react": "^1.2.1",
    "colors": "^1.4.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-formidable": "^1.2.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongoose": "^8.5.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.4",
    "slug": "^9.1.0",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11"
  }
}
